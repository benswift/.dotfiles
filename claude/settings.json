{
  "includeCoAuthoredBy": false,
  "cleanupPeriodDays": 365,
  "env": {
    "DISABLE_TELEMETRY": "1"
  },
  "mcpServers": {
    "ash_ai": {
      "command": "mcp-proxy",
      "args": ["http://localhost:4000/ash_ai/mcp"]
    },
    "tidewave": {
      "command": "mcp-proxy",
      "args": ["http://localhost:4000/tidewave/mcp"]
    }
  }
}
